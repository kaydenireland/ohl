{
  "name": "ohl",
  "displayName": "Ohl",
  "description": "An extension to allow support for the Ohl language, allowing syntax checking and running ohl files from VSCode.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "ohl",
      "aliases": ["Ohl", "ohl"],
      "extensions": [".ohl"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "ohl",
      "scopeName": "source.ohl",
      "path": "./syntaxes/ohl.tmLanguage.json"
    }],
    "iconThemes": [{
      "id": "ohl-file-icons",
      "label": "Ohl File Icons",
      "path": "./icons/ohl-icon-theme.json"
    }]
  }
}
